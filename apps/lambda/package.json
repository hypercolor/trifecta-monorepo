{
  "name": "trifecta-lambda",
  "version": "1.0.0",
  "description": "Async lambda processor.",
  "main": "index.js",
  "license": "NONE",
  "private": true,
  "scripts": {
    "watch": "npm run copy:html && tsc --watch",
    "build": "tsc --build --clean && tsc --build && npm run copy:html",
    "develop": "node -r dotenv/config build/src/entry-points/worker.js",
    "lambda:prepare": "cp build/src/entry-points/index.js .",
    "copy:index": "cp build/entry-points/index.js .",
    "copy:html": "copyfiles -u 1 src/**/*.html build",
    "docker:build": "docker build --platform linux/amd64 -t trifecta-lambda:test -f Dockerfile .",
    "docker:run": "docker run --platform linux/amd64 -p 9000:8080 trifecta-lambda:test"
  },
  "dependencies": {
    "shared-types": "workspace:*",
    "@aws-sdk/client-secrets-manager": "^3.556.0",
    "@aws-sdk/client-sqs": "^3.290.0",
    "@hypercolor/envconfig": "^1.0.15",
    "@hypercolor/promise-queue": "0.0.8",
    "@sendgrid/mail": "^7.7.0",
    "aws-sdk": "^2.780.0",
    "copyfiles": "^2.4.1",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "firebase-admin": "^13.0.2",
    "google-search-results-nodejs": "^2.0.1",
    "md5": "^2.3.0",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "music-metadata": "^10.5.1",
    "node-fetch": "^2.7.0",
    "nodemailer": "^6.9.16",
    "pg": "^8.13.1",
    "pjson": "^1.0.9",
    "reflect-metadata": "^0.1.13",
    "sqs-consumer": "^5.5.0",
    "stripe": "^14.10.0",
    "swagger-ui-express": "^4.1.6",
    "ts-custom-error": "^3.3.1",
    "tsyringe": "^4.7.0",
    "typedjson": "1.8.0",
    "winston": "^3.8.2",
    "winston-papertrail-transport": "^1.0.9"
  },
  "devDependencies": {
    "@types/md5": "^2.3.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "20.9.4",
    "@types/node-fetch": "^2.6.11",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.11.10",
    "nodemon": "^3.0.1",
    "typescript": "^5.2.2"
  }
}
